<%= link_to("<<< Back to Pages list", pages_path, :class => "back-link") %>

<div class="Page edit">
  <h2> Edit Page </h2>

  <%= form_for(@page) do |f| %>

    <%= render(:partial => 'form', :locals => {:f => f})%>

    <div class="form-buttons">
      <%= f.submit("Update Subject") %>
    </div>
  <% end %>
</div>
